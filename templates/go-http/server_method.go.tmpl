            {{- range .Input }}
            reqbody.{{ capitalize .Name }},
            {{- end }}
        )
        if err != nil {
            w.WriteHeader(http.StatusInternalServerError)
            // TODO: Add error!
            return
        }

        {{- if gt (len .Output) 0 }}
        if b, err = json.Marshal(&respbody); err != nil {
            w.WriteHeader(http.StatusInternalServerError)
            // TODO: Add error!
            return
        }
        {{ else }}
