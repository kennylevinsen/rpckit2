// The {{ capitalize .ProtoName }}Methods type defines a server that can
// process requests from the {{ .ProtoName }} protocol.
type {{ capitalize .ProtoName }}Methods interface {
    {{- range .Methods }}
    // {{ capitalize .Description }}
    {{ capitalize .Name }}(ctx context.Context,
    {{- $input := .Input }}
    {{- range $idx, $v := .Input }} req{{ capitalize $v.Name }} {{ $v.T.GoType }}{{- if ne (len $input) $idx }},{{ end }}{{ end }}) (
    {{- range .Output }}resp{{ capitalize .Name }} {{ .T.GoType }}, {{ end }}err error)
    {{- end }}
}
